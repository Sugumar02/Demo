                                <xs:documentation>The description of the port from which the items on the purchase order are shipped. This field is only available when the system is running Import functionality.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="dept" type="xs:int">
                            <xs:annotation>
                                <xs:documentation>Contains the number which uniquely identifies the department.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="dept_name" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Contains the name of the department.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="comment_desc" type="xs:string">
