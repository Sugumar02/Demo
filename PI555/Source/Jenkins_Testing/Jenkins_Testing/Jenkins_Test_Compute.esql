

CREATE COMPUTE MODULE Jenkins_Test_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		 CALL CopyEntireMessage();
		SET Environment.Variables.AuditName = 'Inbound_Audit';
		SET Environment.Variables.BussinessInfo.MessageId = '589_789';
		PROPAGATE to TERMINAL 'out2' DELETE NONE;   -- For Audit Purpose
		
		PROPAGATE TO TERMINAL 'out' DELETE NONE;
		
		SET Environment.Variables.AuditName = 'Outbound_Audit';
		SET Environment.Variables.BussinessInfo.MessageId = '589_789';
		PROPAGATE to TERMINAL 'out' DELETE NONE;   -- For Audit Purpose
		 
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
